FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 202650;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    left
    {
        type            waveTransmissive;
        gamma           1.4;
        value           uniform 202650;
    }
    right
    {
        type            waveTransmissive;
        gamma           1.4;
        value           uniform 202650;
    }
    top
    {
        type            waveTransmissive;
        gamma           1.4;
        value           uniform 202650;
    }
    down
    {
        type            waveTransmissive;
        gamma           1.4;
        value           uniform 202650;
    }
}

